{
	"manifest_version": 3,
	"name": "Cuck to Gentleman",
	"version": "1.6",
	"description": "Replaces the text 'Cuck' with 'Someone more intelligent than me'.",
	"icons": { 
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png" 
	},
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

